{
  "name": "@devramps/cli",
  "version": "0.1.15",
  "description": "DevRamps CLI - Bootstrap AWS infrastructure for CI/CD pipelines",
  "main": "dist/index.js",
  "bin": {
    "devramps": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --watch",
