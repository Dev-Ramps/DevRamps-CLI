  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devramps/cli.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.700.0",
    "@aws-sdk/client-iam": "^3.700.0",
    "@aws-sdk/client-s3": "^3.982.0",
    "@aws-sdk/client-sts": "^3.700.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "express": "^4.21.0",
    "inquirer": "^12.0.0",
    "open": "^10.1.0",
    "ora": "^8.1.0",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
